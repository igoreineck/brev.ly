{
  "extends": "@tsconfig/node23/tsconfig.json",
  "compilerOptions": {
    "lib": ["ES2023"],
    "module": "ESNext",
    "target": "ESNext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
